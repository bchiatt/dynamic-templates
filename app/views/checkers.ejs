<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Dynamic Templates</title>
    <link rel="stylesheet" href="css/style.css">
  </head>
  <body>
    <div>
      <table>
        <% var k = 0 %>
        <% for(var i=0; i<8; i++) { %>
          <tr>
          <% for(var j=0; j<8; j++) { %>
            <% k++ %>
            <% if(k===2||k==4||k==6||k==8||k==9||k==11||k==13||k==15||k==18||k==20||k==22||k==24){var type = 'playerA'}else if(k===41||k==43||k==45||k==47||k==50||k==52||k==54||k==56||k==57||k==59||k==61||k==63){var type = 'playerB'}else{var type = 'blank'} %>
            <td><div class="<%= type %> <%= String(i)+String(j) %> <%= "n" + (i+j) % 2 %>"></div></td>
          <% } %>
          </tr>
        <% } %>
      </table>
    </div>
  </body>
</html>
